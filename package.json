{
  "name": "binance-downloader",
  "version": "0.0.1",
  "description": "Binance data downloader",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start:watch": "nodemon --inspect src/index.ts",
    "start": "npm run build && node ./dist/index.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "keywords": [
    "binance",
    "crypto"
  ],
  "author": "Sajjad Servatjoo",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/luxon": "^3.3.2",
    "@types/node": "^20.8.4",
    "@types/yauzl": "^2.10.1",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "axios-observable": "^2.0.0",
    "enquirer": "^2.4.1",
    "luxon": "^3.4.3",
    "pino": "^8.16.0",
    "pino-pretty": "^10.2.3",
    "rxjs": "^7.8.1",
    "yauzl": "^2.10.0"
  }
